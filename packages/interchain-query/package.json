{
  "name": "interchain-query",
  "version": "1.10.2",
  "description": "Cosmos RPC with React Query",
  "author": "Dan Lynch <pyramation@gmail.com>",
  "homepage": "https://github.com/pyramation/interchain-query#readme",
  "license": "SEE LICENSE IN LICENSE",
  "main": "index.js",
  "typings": "index.d.ts",
  "directories": {
    "lib": "src",
    "test": "__tests__"
  },
  "scripts": {
    "build:cjs": "yarn tsc -p tsconfig.json --outDir main --module commonjs",
    "build:copy": "publish-scripts --cmd copy --srcDir main --outDir dist --findExt js,map,mjs,d.ts --stripPath codegen",
    "clean": "rimraf main && rimraf dist",
    "build": "yarn clean && yarn build:cjs && yarn build:copy",
    "codegen": "node scripts/codegen.js",
    "prepare": "npm run build",
    "lint": "eslint src --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pyramation/interchain-query"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/pyramation/interchain-query/issues"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^4.29.12"
  },
  "devDependencies": {
    "@cosmology/telescope": "^1.4.12",
    "@tanstack/react-query": "^4.29.12",
    "@types/jest": "^29.5.0",
    "cross-env": "^7.0.2",
    "eslint": "8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "jest-in-case": "^1.0.2",
    "prettier": "^2.8.7",
    "publish-scripts": "^1.9.0",
    "regenerator-runtime": "^0.13.11",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "tsup": "^7.1.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@cosmjs/amino": "0.32.2",
    "@cosmjs/proto-signing": "0.32.2",
    "@cosmjs/stargate": "0.32.2",
    "@cosmjs/tendermint-rpc": "0.32.2"
  },
  "gitHead": "e679e23197b229f16d124e49b11e89c61ce94a30"
}
